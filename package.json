  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "npx eas build --platform android",
    "build:android:prod": "npx eas build --platform android --profile production",
    "build:apk": "npx eas build --platform android --profile apk",
    "build:apk:prod": "npx eas build --platform android --profile apk",
    "build:apk:cache": "npx eas build --platform android --profile apk --clear-cache",
    "build:ios": "npx eas build --platform ios",
    "build:ios:prod": "npx eas build --platform ios --profile production",
    "build:all": "npx eas build --platform all",
    "build:all:prod": "npx eas build --platform all --profile production",
    "submit:android": "npx eas submit --platform android",
    "submit:ios": "npx eas submit --platform ios",
    "update": "npx eas update",
    "update:prod": "npx eas update --channel production",
    "optimize": "bash scripts/optimize-production.sh",
    "postbuild": "echo 'âœ… Build completed successfully'"
  },
